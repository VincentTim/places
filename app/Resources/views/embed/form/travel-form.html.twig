
<form name="{{ form.vars.name }}" id="{{ form.vars.id }}" action="{{ path('dashboard_travel_contribute') }}" method="post" enctype="multipart/form-data">
	{{form_row(form.name)}}
	{{form_row(form.period_from)}}
	{{form_row(form.period_to)}}
	{{form_row(form.summary)}}
	{{form_row(form.description)}}
	{{form_row(form.country)}}
	<div class="form-tags">
		{{ form_row(form.tags) }}
		<div class="form-group">
			<ul class="tags"></ul>
		</div>
	</div>
    <div class="form-files">
		{{ form_row(form.files) }}
		<div class="form-group">
			<ul class="files"></ul>
		</div>
	</div>
{{form_end(form)}}
    
{{ tinymce_init() }}